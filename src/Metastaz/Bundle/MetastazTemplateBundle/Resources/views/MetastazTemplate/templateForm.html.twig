{% form_theme form 'MetastazTemplateBundle:Form:fields.html.twig' %}

<form action="{{ path('metastaz_template_create') }}" method="post" {{ form_enctype(form) }}>

    {{ form_row(form.name) }}

    {{ form_rest(form) }}
    <ul class="actions">
        <li><button type="submit">Create</button></li>
        <li><a href="{{ path('metastaz_template') }}">Cancel</a></li>
    </ul>
</form>
