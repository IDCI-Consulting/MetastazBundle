<form action="{{ path('metastaz_template_create') }}" method="post" {{ form_enctype(form) }}>
<fieldset>
    {{ form_widget(form._token) }}
        
    {{ form_label(form.name) }}
    <div class="inputs">{{ form_widget(form.name) }}</div>
</fieldset>
    <p>
        <button type="submit">Create</button>
        <a href="{{ path('metastaz_template') }}">Cancel</a>
    </p>
</form>